{
    "name": "web-root",
    "version": "1.0.0",
    "private": true,
    "type": "module",
    "scripts": {
        "postinstall": "concurrently \"cd client && npm install\" \"cd server && npm install\"",
        "build": "cd client && npm run build",
        "server": "cd server && npm start",
        "client": "cd client && npm run dev",
        "dev": "concurrently \"npm run server\" \"npm run client\""
    },
    "dependencies": {
        "cors": "^2.8.6",
        "dotenv": "^17.2.3",
        "express": "^5.2.1",
        "mongoose": "^9.1.5"
    },
    "engines": {
        "node": "18.x"
    },
    "devDependencies": {
        "concurrently": "^9.2.1"
    }
}
