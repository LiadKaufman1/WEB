{
    "version": 2,
    "rewrites": [
        {
            "source": "/api/get_parents",
            "destination": "/api/get_parents.js"
        },
        {
            "source": "/api/(.*)",
            "destination": "/api/index.js"
        },
        {
            "source": "/((?!api/.*).*)",
            "destination": "/client/index.html"
        }
    ]
}